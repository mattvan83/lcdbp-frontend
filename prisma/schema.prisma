generator nextAdmin {
  provider = "next-admin-generator-prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model contacts {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  v         Int      @map("__v")
  createdAt DateTime @db.Date
  email     String
  firstname String
  lastname  String
  message   String
  ownCopy   Boolean
  phone     String
  updatedAt DateTime @db.Date
}